{
  "name": "compost-culture",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --global script",
    "build": "parcel build src/index.html --global script --public-url https://spectator-static-assets.s3.amazonaws.com/compost-culture --no-content-hash",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint": "^7.0.0",
    "parcel-bundler": "^1.12.3",
    "patch-package": "^6.2.2",
    "posthtml-expressions": "^1.9.0",
    "posthtml-include": "^1.7.4",
    "sass": "^1.26.5"
  },
  "dependencies": {
    "intersection-observer": "^0.10.0",
    "text-balancer": "^1.0.5"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "spectate": {
    "USE_NEWS_NAV": false,
    "USE_EYE_NAV": true,
    "USE_COVER_HED": true,
    "IS_EMBED": false,
    "DOC_URL": "https://docs.google.com/document/d/1sQsZ1hRJ8r-v_ek14yNrqDILDFnvw60ahAHnYfs5QUw/edit"
  }
}
